{
  "name": "shinsekai",
  "version": "0.0.1",
  "description": "Component based visualization framework for Angular",
  "main": "lib/index.js",
  "scripts": {
    "build": "browserify app/index.js -o app/app.js -t [ babelify --optional runtime ]",
    "release": "rm -rf lib && babel src --out-dir lib && npm publish",
    "watch": "watchify app/index.js -o app/app.js -t [ babelify --optional runtime ] -v"
  },
  "author": "Yosuke ONOUE <onoue@likr-lab.com>",
  "license": "MIT",
  "keywords": [
    "angular",
    "visualization"
  ],
  "dependencies": {
    "angular": "^1.4.0"
  },
  "devDependencies": {
    "babelify": "^6.1.2",
    "browserify": "^10.2.1",
    "watchify": "^3.2.1"
  }
}
